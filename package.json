{
  "name": "boilerplate-react",
  "version": "1.0.0",
  "description": "boilerplate for comparing react with angular",
  "main": "index.js",
  "scripts": {
    "start": "clear &&  npm run dev | npm run serve ",
    "serve": "webpack -d --watch --config ./configs/webpack.config-dev.js",
    "dev": "webpack-dev-server --inline --config ./configs/webpack.config-dev.js",
    "test": "clear && karma start ./configs/karma.config.js",
    "build": "NODE_ENV=production webpack -p --config ./configs/webpack.config-prod.js",
    "show-errors": "clear && webpack --display-error-details"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claudiadonea4resilient/boilerplate-react.git"
  },
  "keywords": [
    "boilerplate",
    "react"
  ],
  "author": "Claudia Donea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/claudiadonea4resilient/boilerplate-react/issues"
  },
  "homepage": "https://github.com/claudiadonea4resilient/boilerplate-react#readme",
  "dependencies": {
    "react": "^15.3.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-webpack": "^6.4.3",
    "babel-preset-react": "^6.3.13",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-server": "^0.9.0",
    "ip": "^1.1.3",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "jshint": "^2.9.4",
    "jsx-loader": "^0.13.2",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "phantom": "^3.0.2",
    "phantomjs": "^2.1.7",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-validator": "^2.2.9"
  }
}
